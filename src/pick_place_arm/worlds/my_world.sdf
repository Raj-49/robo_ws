          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000058</ixx>
            <iyy>0.000058</iyy>
            <izz>0.000020</izz>
          </inertia>
        </inertial>
        <sensor name="red_box_contact" type="contact">
          <always_on>1</always_on>
          <update_rate>100</update_rate>
          <contact>
            <collision>red_collision</collision>
          </contact>
          <visualize>0</visualize>
          <topic>/red_box/contacts</topic>
        </sensor>
      </link>
    </model>

